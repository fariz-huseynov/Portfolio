{
  "stryker-config": {
    "project": "Portfolio.Infrastructure.csproj",
    "reporters": ["html", "cleartext", "progress"],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 30
    },
    "mutate": [
      "!**/obj/**",
      "!**/bin/**",
      "!**/Migrations/**",
      "!**/Configurations/**",
      "Caching/HybridCacheService.cs",
      "Identity/CaptchaService.cs",
      "Services/HtmlSanitizerService.cs"
    ]
  }
}
